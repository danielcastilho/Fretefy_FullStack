# EditorConfig is awesome: https://EditorConfig.org

# top-most EditorConfig file
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = false
insert_final_newline = false

[*.cs]
end_of_line = lf
indent_size = 4
indent_style = space
trim_trailing_whitespace = true
max_line_length = 120
insert_final_newline = true

